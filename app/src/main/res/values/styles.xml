<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="AppBaseTheme1" parent="Theme.AppCompat.Light">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="colorPrimary">@color/blue_color</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
    </style>


    <style name="AppThemeActivity" parent="AppBaseTheme1"></style>

    <style name="ActionBar" parent="ThemeOverlay.AppCompat.ActionBar">
        <item name="android:displayOptions">homeAsUp</item>
    </style>

    <style name="AppTheme" parent="Theme.AppCompat.NoActionBar">

        <item name="android:typeface">monospace</item>
    </style>

    <style name="appbarTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="android:textColorPrimary">#ffffff</item>
        <item name="actionMenuTextColor">#FFFFFF</item>
        <item name="android:textColorSecondary">#ffffff</item>
        <item name="android:background">@color/blue_color</item>
    </style>

    <!-- New Add Appbar theme -->

    <style name="appbarThemeBlack" parent="Theme.AppCompat.NoActionBar">
        <item name="android:textColorPrimary">#ffffff</item>
        <item name="actionMenuTextColor">#FFFFFF</item>
        <item name="android:textColorSecondary">#ffffff</item>
        <!-- <item name="android:background">@color/green</item> -->
    </style>

    <style name="GalaxyZooThemeToolbarDarkOverflow" parent="Theme.AppCompat.NoActionBar">
        <item name="actionMenuTextColor">@color/white</item>
    </style>

    <style name="menu_text_style" parent="@android:style/TextAppearance.DeviceDefault.Widget.ActionBar.Menu">
        <item name="android:textSize">25sp</item>
        <item name="android:textColor">@color/blue_color</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="BMHEditTextStyle" parent="@android:style/Widget.EditText">
        <item name="android:textColor">#000000</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_marginTop">5dp</item>
    </style>

    <style name="tv_otp_msg">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="tv_otp_msg_2">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="tv_otp_msg1">
        <item name="android:textColor">@color/grey</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="BHMTextVIewStyle" parent="@android:style/Widget.TextView">
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="BHMButtonStyle">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">18sp</item>
        <item name="android:layout_height">45dp</item>
    </style>

    <style name="MineCustomTabText" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">12sp</item>
    </style>

    <style name="AmenitiesTvStyle" parent="@android:style/Widget.TextView">
        <item name="android:textColor">#5E5E5E</item>
        <item name="android:textSize">12sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:gravity">center</item>
        <item name="android:maxWidth">80dp</item>
    </style>

    <style name="briceBreakTvStyle" parent="@android:style/Widget.TextView">
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">left</item>
    </style>

    <style name="DialogAnimation">
        <item name="android:windowEnterAnimation">@android:anim/slide_in_left</item>
        <item name="android:windowExitAnimation">@android:anim/slide_out_right</item>
    </style>

    <style name="llAmenities">
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">90dp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/buy_rent_selector</item>
        <item name="android:orientation">vertical</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingLeft">10dp</item>
    </style>

    <style name="Divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/bg_grey</item>
    </style>

    <style name="DividerVertical">
        <item name="android:layout_width">1dip</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/bg_grey</item>
    </style>

    <style name="Divider_Vertical">
        <item name="android:layout_width">1dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/bg_grey</item>
    </style>


    <style name="MyDialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <style name="progress_dialog_theme">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

